{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"project": {
			"type": "object",
			"properties": {
				"TimeSaved": {
					"type": "string"
				},
				"RVersion": {
					"type": "string"
				},
				"RstoxPackageVersion": {
					"type": "array",
					"minItems": 3,
					"items": {
						"type": "string"
					}
				},
				"CertifiedRstoxPackageVersion": {
					"type": "array",
					"minItems": 3,
					"items": {
						"type": "string"
					}
				},
				"AllCertifiedRstoxPackageVersion": {
					"type": "boolean"
				},
				"OfficialRstoxFrameworkVersion": {
					"type": "boolean"
				},
				"DependentPackageVersion": {
					"type": "array",
					"minItems": 1,
					"items": {
						"type": "string"
					}
				},
				"Application": {
					"type": "string"
				}, 
				
				"models": {
					"type": "object",
					"properties": {
						"baseline": {
							"$ref": "#/model"
						}, 
						"analysis": {
							"$ref": "#/model"
						}, 
						"report": {
							"$ref": "#/model"
						}
					}, 
					"required": [
						"baseline",
						"analysis",
						"report"
					]
				}
						
			},
			"required": [
				"TimeSaved",
				"RVersion",
				"RstoxPackageVersion",
				"CertifiedRstoxPackageVersion",
				"AllCertifiedRstoxPackageVersion",
				"OfficialRstoxFrameworkVersion",
				"DependentPackageVersion",
				"Application"
			],
			"additionalProperties": false
		}
	},"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"project": {
			"type": "object",
			"properties": {
				"TimeSaved": {
					"type": "string"
				},
				"RVersion": {
					"type": "string"
				},
				"RstoxPackageVersion": {
					"type": "array",
					"minItems": 3,
					"items": {
						"type": "string"
					}
				},
				"CertifiedRstoxPackageVersion": {
					"type": "array",
					"minItems": 3,
					"items": {
						"type": "string"
					}
				},
				"AllCertifiedRstoxPackageVersion": {
					"type": "boolean"
				},
				"OfficialRstoxFrameworkVersion": {
					"type": "boolean"
				},
				"DependentPackageVersion": {
					"type": "array",
					"minItems": 1,
					"items": {
						"type": "string"
					}
				},
				"Application": {
					"type": "string"
				}, 
				
				"models": {
					"type": "object",
					"properties": {
						"baseline": {
							"$ref": "#/model"
						}, 
						"analysis": {
							"$ref": "#/model"
						}, 
						"report": {
							"$ref": "#/model"
						}
					}, 
					"required": [
						"baseline",
						"analysis",
						"report"
					]
				}
						
			},
			"required": [
				"TimeSaved",
				"RVersion",
				"RstoxPackageVersion",
				"CertifiedRstoxPackageVersion",
				"AllCertifiedRstoxPackageVersion",
				"OfficialRstoxFrameworkVersion",
				"DependentPackageVersion",
				"Application",
				"models"
			],
			"additionalProperties": false
		}
	},
	
	"model": {
		"type": "array",
		"items": {
			"$ref": "#/process"
		}
	},
	
	
	"process": {
		"type": "object",
		"properties": {
			"processName": {
				"type": "string"
			},
			"functionName": {
				"type": "string", 
				"minLength": 0
			},
			"functionParameters": {},
			"functionInputs": {},
			"processParameters": {
			    "type": "object",
			    "properties": {
			    	"enabled": {
						"type": "boolean"
				    },
				    "showInMap": {
				    	"type": "boolean"
			    	},
			    	"fileOutput": {
		    			"type": "boolean"
		    		}
		    	},
		    	"required": [
		   	    	"enabled",
		 	   		"showInMap",
			    	"fileOutput"
			    ]
			},
			"processData": {
				"$ref": "#/processData"
			}
		},
		"required": [
		"functionInputs",
		"functionName",
		"functionParameters",
		"processData",
		"processName",
		"processParameters"
		]
	},
	
		
	"required": [
		"project"
	]
}